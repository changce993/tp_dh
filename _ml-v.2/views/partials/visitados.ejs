<% for(var i=0; i<visitados.length; i++) { %>
    <div class="col-12 col-sm-6 col-lg-3">
        <section class="product-box">
            <a href="/product/detail/<%= visitados[i].id%>/<%= visitados[i].category%>"%></a>
                <figure class="product-box_image">
                    <img src="/images/products/<%= visitados[i].image %>" alt="imagen de producto">
                </figure>
                <article class="product-box_data">
                    <h2><%= visitados[i].price %></h2>
                    <span>40% OFF</span>
                    <p><%= visitados[i].name %> Lorem ipsum, dolor sit amet.</p>
                    <i class="fas fa-truck"></i>
                </article>
            </a>
        </section>
    </div>
<% } %>